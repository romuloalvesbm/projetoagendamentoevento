
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    
    <title>Sistema de Agendamento</title>

    <link href="~/css/bootstrap.css" rel="stylesheet" />
    <link href="~/css/StyleTemplate.css" rel="stylesheet" />

    <style type="text/css">
        .card-container {
            max-width: 350px;
            padding: 50px 50px;
        }

        .card {
            background-color: #F7F7F7;
            margin: 0 auto 25px;
            margin-top: 30px;
            margin-bottom: 20px;
            -moz-border-radius: 2px;
            -webkit-border-radius: 2px;
            border-radius: 2px;
            -moz-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);
            -webkit-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);
            box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);
        }

        .profile-img-card {
            width: 96px;
            height: 96px;
            margin: 0 auto 10px;
            display: block;
            -moz-border-radius: 15%;
            -webkit-border-radius: 15%;
            border-radius: 15%;
        }
    </style>

</head>
<body>
    <header>
        <div class="container-fluid cabecalho">
            <div class="row vertical-center">
                <div style="padding-left: 15px; width: 100%">
                    <img alt="Logo-MAN" src="../Style/Image/Logo_Topo.png" class="img-fluid" />
                </div>
            </div>
        </div>
    </header>

    <div class="container-fluid">
        <div class="card card-container">
            <img id="profile-img" class="profile-img-card" src="../Style/Image/LoginUser.png" />
            <div class="card-body">

               Deu ruim

            </div>
        </div>
    </div>
    

    <footer class="footer">
        <div class="container-fluid">
            <span class="text-muted">Sistema AGE - © Copyright 2020 - Todos os direitos reservados.</span>
        </div>
    </footer>

</body>
</html>
